    "        ax.axhline(0.0, linestyle='--', color='k')\n",
    "        ax.axvline(np.mean(self.yield_data - self.reg_pred), linestyle='-', color='g', label='Mean')\n",
    "        ax.axvline(np.median(self.yield_data - self.reg_pred), linestyle='-', color='r', label='Median')\n",
    "\n",
    "        # Plot residuals\n",
    "        xshift = 0\n",
    "        for num, resi_lst in enumerate(self.resi):\n",
    "            ax.scatter(np.linspace(1 + xshift, len(resi_lst) + xshift, len(resi_lst)), resi_lst, marker='+')\n",
    "            xshift += len(resi_lst)\n",
    "\n",
    "        ax.plot(np.linspace(1, self.resi.shape[0] * self.resi.shape[1] + 1, self.resi.shape[0]),\n",
    "                np.mean(self.resi, axis=1), label='Region Mean')\n",
    "\n",
    "        # Label axes\n",
    "        ax.set_xlabel(r'Run')\n",
    "        ax.set_ylabel('Residual ' + self.metric + ' (' + self.metric_units + ')')\n",
    "\n",
    "        handles, labels = ax.get_legend_handles_labels()\n",
    "        ax.legend(handles, labels)\n",
    "\n",
    "        fig.savefig('model_performance/region_residuals_' + self.metric + '.png', dpi=300, bbox_inches='tight')\n",
    "\n",
    "        fig.clf()\n",
    "\n",
