  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Compile the Stan model\n",
    "gm = pystan.StanModel(file='Stan_models/2d-gaussian_with_correlation_anom.stan')"
